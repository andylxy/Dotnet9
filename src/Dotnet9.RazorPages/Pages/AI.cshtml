@page "/ai"

@inject ISystemClientService SystemClientService
@{
    var siteInfo = await SystemClientService.GetSiteInfoAsync();
    Layout = "Shared/_CustomLayout";
    ViewData[ViewDataKeys.Title] = "Token AI";
}

@section Headers
{
    <style>
        * {
            margin: 0;
            padding: 0
        }

        body {
            overflow: hidden;
        }

        iframe{
            position: absolute;
            width: 100%;
            height: 100%;
            }

    </style>
}

<iframe src="https://open666.cn?home=@siteInfo?.Domain"></iframe>