@page "/albums/{Slug}"

@model Dotnet9.RazorPages.Pages.Blogs.SearchModel
@{
    ViewData[ViewDataKeys.Title] = $"专辑-{Model.RequestResponse?.AlbumName}";
    ViewData[ViewDataKeys.Keywords] = Model.Slug;
    ViewData[ViewDataKeys.Description] = Model.RequestResponse?.AlbumName;
}

@await Html.PartialAsync("_PaginationPartial", new PaginationModel(Model.Blogs, $"/albums/{Model.Slug}", Model.Current, Model.Pages, Model.RequestResponse.Total, Model.PageSize, Model.RequestResponse.TotalPage))